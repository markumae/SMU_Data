Categories <- c("computer programing", "math", "statistics", "machine learning, "domain expertise", "communication and presentation skills", "data visualization")
Ranking <- c(1, 2, 3, 4, 5, 6, 7)
Amy <- data.frame(Categories, Ranking)
gplot(Amy, aes(1:7, Categories, Ranking)) + xlab("Ranking") + ylab("Categories")
